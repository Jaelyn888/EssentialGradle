        <item name="android:layout_height">45dp</item>
        <item name="android:layout_marginLeft">@dimen/common_double_margin</item>
        <item name="android:layout_marginRight">@dimen/common_double_margin</item>
        <item name="android:textSize">@dimen/font_size16</item>
    </style>

    <!-- 灰色按钮 -->
    <style name="grey_bt_style">
        <item name="android:background">@drawable/cus_grey_border_frame</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- 订单操作 -->
    <style name="user_order_option_bt_style">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">@dimen/user_order_option_bt_width</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/darkGray</item>
        <item name="android:layout_marginLeft">@dimen/user_order_option_margin</item>
        <item name="android:layout_marginRight">@dimen/user_order_option_margin</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/cus_white_grey_bt</item>
    </style>

    <!-- 订单列表操作订单的样式 -->
    <style name="user_order_list_option_bt_style">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">65dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/darkgray_gray_txt</item>
        <item name="android:layout_marginLeft">@dimen/common_margin</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/cus_white_grey_bt</item>
    </style>

    <!-- 验证码限制 -->
    <style name="edit_msg_code_style" parent="edit_login_reg_style">
        <item name="android:inputType">number</item>
    </style>


    <style name="common_navi_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <!-- 订单类别的样式 -->

    <!-- 个人中心页 -->
    <style name="navi_style" parent="common_navi_style">
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="navi_tv_style">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
    </style>
    <!-- 消息数量提示 -->
    <style name="num_msg_style">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@drawable/light_red_circle</item>
    </style>

    <!-- 水平分割线样式  imageview -->
    <style name="hor_line_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/line_height</item>
        <item name="android:src">@drawable/hor_line</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="vertical_line_style">
        <item name="android:layout_width">@dimen/line_height</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:src">@drawable/vertical_line</item>
    </style>


    <!-- 相关商品gridview样式 -->
    <style name="relation_product_style">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:numColumns">@integer/product_coloumnNum</item>
        <item name="android:listSelector">@color/transparent</item>
        <item name="android:cacheColorHint">@color/transparent</item>
        <item name="android:paddingTop">@dimen/common_margin</item>
        <item name="android:paddingBottom">@dimen/common_margin</item>
        <item name="android:layout_marginLeft">@dimen/common_margin</item>
        <item name="android:layout_marginRight">@dimen/common_margin</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:horizontalSpacing">@dimen/common_margin</item>
        <item name="android:verticalSpacing">@dimen/common_margin</item>
        <item name="android:background">@color/white</item>
        <item name="android:scrollbars">none</item>
    </style>

    <!-- dialogActivity 样式 -->
    <style name="Transparent" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:background">@drawable/btn_shape_white_no</item>
        <!-- dialog背景样式 -->
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>

    <style name="activity_page">
        <item name="android:splitMotionEvents">false</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="topbar_title">
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">18sp</item>
    </style>


</resources>