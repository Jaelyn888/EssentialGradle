package com.yishanxiu.yishang.view;

import android.content.Context;
import android.transition.Transition;
import android.util.AttributeSet;
import android.view.View;
import android.widget.PopupWindow;

/**
 * Created by Jaelyn on 2016/9/12.
 */
public class BottomPopWindow extends PopupWindow {
	public BottomPopWindow(Context context) {
		super(context);
	}

	public BottomPopWindow(Context context, AttributeSet attrs) {
		super(context, attrs);
	}

	public BottomPopWindow(Context context, AttributeSet attrs, int defStyleAttr) {
		super(context, attrs, defStyleAttr);
	}

	public BottomPopWindow(Context context, AttributeSet attrs, int defStyleAttr, int defStyleRes) {
		super(context, attrs, defStyleAttr, defStyleRes);
	}

	public BottomPopWindow() {
		super();
	}

	public BottomPopWindow(View contentView) {
		super(contentView);
	}

	public BottomPopWindow(int width, int height) {
		super(width, height);
	}

	public BottomPopWindow(View contentView, int width, int height) {
		super(contentView, width, height);
	}

	public BottomPopWindow(View contentView, int width, int height, boolean focusable) {
		super(contentView, width, height, focusable);
	}
}
