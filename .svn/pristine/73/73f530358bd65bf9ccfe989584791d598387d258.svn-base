package com.yishanxiu.yishang.getdata;

/**
 * 获得服务器数据的json中的key字段
 * 
 * @author 亚明
 * 
 */
public class JsonKeys {
	/**
	 * 本次访问的状态码
	 */
	public final static String Key_Code = "code";
	/**
	 * 本次访问的状态消息
	 */
	public final static String Key_Msg = "msg";
	/**
	 * 本次访问的数据信息
	 */
	public final static String Key_Info = "info";

	/**
	 * json数据中jsonObject对象中的id
	 */
	public final static String Key_Objid = "id";
	/**
	 * json数据中jsonObject对象中的Id
	 */
	public final static String Key_ObjId = "Id";
	/**
	 * json数据中jsonObject对象中的Name
	 */
	public final static String Key_ObjName = "Name";
	/**
	 * json数据中jsonObject对象中的name
	 */
	public final static String Key_Objname = "name";
}
