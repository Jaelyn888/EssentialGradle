package com.yishanxiu.yishang.model;

import java.util.List;

/**
 * Created by Jaelyn on 2016/9/18.
 *
 */
public class ShopDetailResponse {
	private BrandInfo brand;
	private List<ProductBaseInfo> productList;
	//是否收藏
	private int collectionFlag;

	public int getCollectionFlag() {
		return collectionFlag;
	}

	public void setCollectionFlag(int collectionFlag) {
		this.collectionFlag = collectionFlag;
	}

	public BrandInfo getBrand() {
		return brand;
	}

	public void setBrand(BrandInfo brand) {
		this.brand = brand;
	}

	public List<ProductBaseInfo> getProductList() {
		return productList;
	}

	public void setProductList(List<ProductBaseInfo> productList) {
		this.productList = productList;
	}
}
